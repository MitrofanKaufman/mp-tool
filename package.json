{
  "name": "ws-api-server",
  "version": "1.0.0",
  "description": "WebSocket API server for Wildberries integration",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "migrate": "node server/utils/migrate.js",
